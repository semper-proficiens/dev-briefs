package components

templ MetaDiv() {
    <div class="flex flex-col items-left">
        <div>
           <img class="bg-inherit" src="assets/Meta.png" alt="meta" width="300" height="100"/>
        </div>
        <div class="container p-4">
            @LoadButton("Meta", "meta", "meta-container")
            @CollapseDivButton("news-container")
            <div id="news-meta">
                <!-- News content will be loaded here -->
            </div>
        </div>
    </div>
}